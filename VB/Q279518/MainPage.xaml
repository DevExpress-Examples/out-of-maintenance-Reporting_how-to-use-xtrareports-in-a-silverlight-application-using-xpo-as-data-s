<UserControl xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/printing" x:Class="Q279518.MainPage" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" d:DesignHeight="300" d:DesignWidth="400">

    <Grid x:Name="LayoutRoot" Background="White">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Button Grid.Row="1" Content="Show Report" Click="OnShowReportButtonClick" />
        <dxp:DocumentPreview Grid.Row="0" AutoCreateDocument="False" Name="preview">
            <dxp:DocumentPreview.Model>
                <dxp:ReportPreviewModel ServiceUri="../ReportService.svc" ReportTypeName="Q279518.Web.SuppliersReport" />
            </dxp:DocumentPreview.Model>
        </dxp:DocumentPreview>
     </Grid>
</UserControl>
